@model IEnumerable<Airoport.Models.Client>

<div id="searchresults">

    <h2>Совпадения: @ViewBag.Err </h2>
    <ul>
        @foreach (var item in Model)
        {
            <li>
                <p>
                    @item.Name
                    @item.Surname
                    <a href="/Ticket/EditClientList/@item.Id/@ViewData["config"]">  Регистрация посадки</a>
                </p>
            </li>
        }
    </ul>

</div>
